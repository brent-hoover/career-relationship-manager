generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Job {
  id          Int       @id @default(autoincrement())
  position    String
  company     String
  maxSalary   Int
  location    String
  status      String
  dateSaved   DateTime
  deadline    DateTime?
  dateApplied DateTime?
  followUp    DateTime?
  excitement  Float
  createdAt   DateTime  @default(now())
  updatedAt   DateTime  @updatedAt
}
